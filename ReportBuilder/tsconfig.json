{
  "compilerOptions": {
    "outDir": "./wwwroot/dist/ts",
    "sourceMap": true,
    "strict": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "experimentalDecorators": true,
    "declaration": true,
    "emitDecoratorMetadata": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "compileOnSave": false,
  "include": [
    "./src/**/*"
  ]
}
